{
    "cSpell.words": [
        "libgtest",
        "MINISSD",
        "valgrind"
    ],
    "files.associations": {
        "*.smr": "scheme",
        "*.fnl": "scheme",
        "*.urn": "scheme",
        "*.prf": "ini",
        "*.dhall": "haskell",
        "bsconfig.json": "jsonc",
        "minissd.h": "c",
        "chrono": "cpp",
        "functional": "cpp",
        "optional": "cpp",
        "system_error": "cpp",
        "type_traits": "cpp",
        "xlocmon": "cpp",
        "xlocnum": "cpp",
        "xtr1common": "cpp"
    },
    "clang-tidy.compilerArgs": [
        "-Iinclude",
        "-std=gnu99"
    ],
    "editor.formatOnType": true,
    "[commonlisp]": {
        "editor.wordSeparators": "`|;:'\",()"
    }
}